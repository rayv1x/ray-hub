local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "rays hub", HidePremium = false, IntroText = "Rays Hub!", SaveConfig = true, ConfigFolder = "ray#0007"})

